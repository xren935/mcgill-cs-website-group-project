', checkScrollTop)\n\n  return (\n        <FaArrowCircleUp className=\"scrollTop\" onClick={scrollTop} style={{height: 40, alignItems:\"right\", display: showScroll ? 'flex' : 'none'}}/>\n  );\n}\n\nexport default ScrollArrow;","import React, { Component } from 'react';\nimport { FaDribbble, FaFacebook, FaInstagramSquare, FaTwitter } from \"react-icons/fa\";\nimport \"bootstrap/dist/css/bootstrap.min.css\";\nimport './static/footer.css'\nclass Footer extends Component {\n    constructor(props) {\n        super(props);\n        this.state = {}\n    }\n    render() { \n        return (  \n            <footer class=\"site-footer\">\n                <div class=\"container\">\n                    <div class=\"row\" style={{paddingLeft: \"46px\"}}>\n                        <div class=\"col-sm-12 col-md-6\" id=\"footerLogo\" style={{paddingLeft: \"20px\"}}>\n                        </div>\n                        <div class=\"col-xs-6 col-md-1\">\n                        </div>\n                        <div class=\"col-xs-6 col-md-3\">\n                            <h6><b>Quick Links</b></h6>\n                            <ul class=\"footer-links\">\n                            <li><a href=\"\">About Us</a></li>\n                            <li><a href=\"\">Contact Us</a></li>\n                            <li><a href=\"\">Contribute</a></li>\n                            </ul>\n                        </div>\n                        <div class=\"col-xs-6 col-md-2\">\n                            <h6><b>About</b></h6>\n                            <ul class=\"footer-links\">\n                            <li><a href=\"\">McGill CS</a></li>\n                            </ul>\n                        </div> \n                    </div>\n                    <div class=\"container\">\n                        <div class=\"row\">\n                        <div class=\"col-md-8 col-sm-6 col-xs-12\">\n                            <a href=\"#\" class=\"copyright-text\">Copyright &copy; McGi